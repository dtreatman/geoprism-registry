          </mdBusinessPermission>
        </grant>
      </role>
      <role roleName="commongeoregistry.APIConsumer">
        <grant>
          <mdBusinessPermission type="net.geoprism.registry.MasterList">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
        </grant>
