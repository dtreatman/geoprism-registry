      <mdBusiness name="net.geoprism.georegistry.action.DeleteAttributeTypeAction" extends="net.geoprism.georegistry.action.AbstractAction"></mdBusiness>
      -->
      
      <!-- Term -->
      <!--
      <mdBusiness name="net.geoprism.georegistry.action.CreateTermAction" extends="net.geoprism.georegistry.action.AbstractAction"></mdBusiness>
      <mdBusiness name="net.geoprism.georegistry.action.UpdateTermAction" extends="net.geoprism.georegistry.action.AbstractAction"></mdBusiness>
      <mdBusiness name="net.geoprism.georegistry.action.DeleteTermAction" extends="net.geoprism.georegistry.action.AbstractAction"></mdBusiness>
      -->
      
      <mdBusiness name="net.geoprism.georegistry.action.tree.AddChildAction" exported="false" extends="net.geoprism.georegistry.action.AbstractAction">
        <attributes>
          <text name="childId" label="childId" required="true" />
          <text name="childTypeCode" label="childTypeCode" required="true" />
          <text name="parentId" label="parentId" required="true" />
          <text name="parentTypeCode" label="parentTypeCode" required="true" />
          <text name="hierarchyTypeCode" label="hierarchyTypeCode" required="true" />
        </attributes>
      </mdBusiness>
      <mdBusiness name="net.geoprism.georegistry.action.tree.RemoveChildAction" exported="false" extends="net.geoprism.georegistry.action.AbstractAction">
        <attributes>
          <text name="childId" label="childId" required="true" />
          <text name="childTypeCode" label="childTypeCode" required="true" />
          <text name="parentId" label="parentId" required="true" />
          <text name="parentTypeCode" label="parentTypeCode" required="true" />
          <text name="hierarchyTypeCode" label="hierarchyTypeCode" required="true" />
        </attributes>
      </mdBusiness>
    </create>
    <update></update>
  </doIt>
  <undoIt>
    <delete>
      <object key="net.geoprism.georegistry.action.AllGovernanceStatus" type="com.runwaysdk.system.metadata.MdEnumeration"/>
      <object key="net.geoprism.georegistry.action.ChangeRequest" type="com.runwaysdk.system.metadata.MdBusiness"/>
      <object key="net.geoprism.georegistry.action.HasActionRelationship" type="com.runwaysdk.system.metadata.MdRelationship"/>
      <object key="net.geoprism.georegistry.action.AbstractAction" type="com.runwaysdk.system.metadata.MdBusiness"/>
      <object key="net.geoprism.georegistry.action.geoobject.CreateGeoObjectAction" type="com.runwaysdk.system.metadata.MdBusiness"/>
      <object key="net.geoprism.georegistry.action.geoobject.UpdateGeoObjectAction" type="com.runwaysdk.system.metadata.MdBusiness"/>
      <object key="net.geoprism.georegistry.action.tree.AddChildAction" type="com.runwaysdk.system.metadata.MdBusiness"/>
    </delete>
