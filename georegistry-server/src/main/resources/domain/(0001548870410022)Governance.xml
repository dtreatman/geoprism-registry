<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <enumerationMaster
        name="net.geoprism.georegistry.action.GovernanceStatus"
        label="Status"
        generateController="false" />
      
      <object
        key="ACCEPTED"
        type="net.geoprism.georegistry.action.GovernanceStatus">
        <attribute
          name="enumName"
          value="ACCEPTED" />
        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="ACCEPTED" />
        </attributeStruct>
      </object>
      
      <object
        key="REJECTED"
        type="net.geoprism.georegistry.action.GovernanceStatus">
        <attribute
          name="enumName"
          value="REJECTED" />
        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="REJECTED" />
        </attributeStruct>
      </object>
      
      <object
        key="PENDING"
        type="net.geoprism.georegistry.action.GovernanceStatus">
        <attribute
          name="enumName"
          value="PENDING" />
        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="PENDING" />
        </attributeStruct>
      </object>
      
      <mdEnumeration
        name="net.geoprism.georegistry.action.AllGovernanceStatus"
        type="net.geoprism.georegistry.action.GovernanceStatus"
        label="All Governance Statuses" exported="false" publish="false">
        <includeAll />
      </mdEnumeration>
    
      <mdBusiness name="net.geoprism.georegistry.action.ChangeRequest" exported="false" publish="false" label="Execute Actions Request">
        <attributes>
          <enumeration name="approvalStatus" label="Status" selectMultiple="false" type="net.geoprism.georegistry.action.AllGovernanceStatus"/>
        </attributes>
      </mdBusiness>
      
      <mdRelationship name="net.geoprism.georegistry.action.HasActionRelationship" label="Has Action Relationship" generateController="false" exported="false" publish="false">
        <parent cardinality="1" name="net.geoprism.georegistry.action.ChangeRequest" method="Request" />
        <child cardinality="*" name="net.geoprism.georegistry.action.AbstractAction" method="Action" />
      </mdRelationship>
    
      <mdBusiness name="net.geoprism.georegistry.action.AbstractAction" isAbstract="true" exported="false" publish="false" label="Registry Action">
        <attributes>
          <dateTime name="createActionDate" required="true" label="The date time that the action was created (on the remote client)." />
          <text name="apiVersion" required="true" label="Registry API Version" />
          <enumeration name="approvalStatus" label="Status" selectMultiple="false" type="net.geoprism.georegistry.action.AllGovernanceStatus"/>
        </attributes>
      </mdBusiness>
      
      <!-- GeoObject -->
      <mdBusiness name="net.geoprism.georegistry.action.geoobject.CreateGeoObjectAction" exported="false" extends="net.geoprism.georegistry.action.AbstractAction">
        <attributes>
          <text name="geoObjectJson" label="geoObjectJson" required="true" />
        </attributes>
      </mdBusiness>
      <mdBusiness name="net.geoprism.georegistry.action.geoobject.UpdateGeoObjectAction" exported="false" extends="net.geoprism.georegistry.action.AbstractAction">
        <attributes>
          <text name="geoObjectJson" label="geoObjectJson" required="true" />
        </attributes>
      </mdBusiness>
      
      <!-- AttributeType -->
      <!-- TODO : RegistryUrls.GEO_OBJECT_TYPE_ADD_ATTRIBUTE   has two different methods mapping to it -->
